@{
    Layout = null;

    if (Session["userID"] == null)
    {
        Response.Redirect(@Url.Action("Index", "Login"));
    }
}

@Styles.Render("~/Login/css")

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>NES Page Not Found</title>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col mt-3">
                <div class="d-flex justify-content-center"><img src="~/Images/page-not-found-002.png" class="w-75 mt-5" /></div>
                <h1 class="display-4 text-black-50 text-center mt-5">The page you are looking for is not found!</h1>
            </div>
        </div>
        <div class="d-flex mt-5">
            <a href="@Url.Action("Index", "Home" )" class="btn btn-primary m-auto" data-toggle="tooltip" title="Go to Application Home"><i class="mdi mdi-home-outline mdi-24px"></i> Home</a>
        </div>
    </div>

    @Scripts.Render("~/Login/js")

    <script type="text/javascript">
        $(document).ready(function () {
            $('[data-toggle="tooltip"]').tooltip();
        });
    </script>
</body>
</html>